import base.Readers;

/* integer */
println("Integer: " + Readers.readInt("What's your diameter?"));
println("Integer: " + Readers.readInt());

/* double */
println("Double: " + Readers.readDouble("What's your foot's length?"));
println("Double: " + Readers.readDouble());

/* character */
println("Character: " + Readers.readChar("Give me a character!"));
println("Character: " + Readers.readChar());

/* string */
println("String: " + Readers.readString("What's your name?"));
println("String: " + Readers.readString());

exit();